{
  "version": 2,
  "builds": [
    {
      "src": "*.php",
      "use": "@vercel/php@0.6.0"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/simple_demo.php"
    },
    {
      "src": "/admin",
      "dest": "/simple_admin.php"
    },
    {
      "src": "/demo",
      "dest": "/simple_demo.php"
    }
  ]
} 